{
  "name": "pokedex",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://classicaljo.github.io/pokedex",
  "dependencies": {
    "gh-pages": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "cc": "concurrently -k 'npm:start' 'npm:sass'",
    "sass": "sass --watch src/assets/scss:src/assets/css ",
    "start": "react-scripts start & sass --watch src/assets/scss:src/assets/css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-devtools-extension": "^2.13.8",
    "sass": "^1.25.0"
  }
}
